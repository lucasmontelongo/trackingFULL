@model IEnumerable<WEBLayer2.Models.PuntoControl>

@{
    ViewBag.Title = "Details";
}
<link rel="stylesheet" href="~/Content/style.css" type="text/css" />
<link rel="stylesheet" href="~/Content/animate.css" type="text/css" />
<link rel="stylesheet" href="~/Content/select2.min.css" type="text/css" />
<link rel="stylesheet" href="~/Content/perfect-scrollbar.css" type="text/css" />
<link rel="stylesheet" href="~/Content/util.css" type="text/css" />
<link rel="stylesheet" href="~/Content/main.css" type="text/css" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
<link href="https://fonts.googleapis.com/css?family=Oswald&display=swap" rel="stylesheet">

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
<script src="~/Scripts/popper.js" type="text/javascript"></script>
<script src="~/Scripts/select2.min.js" type="text/javascript"></script>
<script src="~/Scripts/main.js" type="text/javascript"></script>

<link rel="stylesheet" href="~/Content/tabla_clientes.css" type="text/css" />
<link rel="stylesheet" type="text/css" href="~/Content/jquery.dataTables.min.css">
<script type="text/javascript" charset="utf8" src="~/Scripts/detalleTrayecto.js"></script>
<script type="text/javascript" charset="utf8" src="~/Scripts/jquery.dataTables.js"></script>

<div>
    <button id="volver">
        @Html.ActionLink("Volver", "Index")
    </button>
</div>
<h2 style="padding: 2% 0 2% 0; color: white">Trayecto @ViewBag.TRAYECTO.Nombre</h2>
<h3 style="padding: 2% 0 2% 0; color: white; text-align: center">Puntos de control</h3>

<div class="container-table100">
    <div class="wrap-table100">
        <div class="table100 ver1 m-b-110">
            <table id="detalleTrayecto" class="table display" data-vertable="ver1">
                <thead>
                    <tr class="row100 head">
                        <th class="column100 column1" data-column="column1">
                            @Html.DisplayNameFor(model => model.Orden)
                        </th>
                        <th class="column100 column2" data-column="column2">
                            @Html.DisplayNameFor(model => model.Nombre)
                        </th>
                        <th class="column100 column3" data-column="column3">
                            @Html.DisplayNameFor(model => model.Tiempo)
                        </th>
                        <th class="column100 column4" data-column="column4">
                            @Html.DisplayNameFor(model => model.IdAgencia)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr class="row100">
                            <td class="column100 column1" data-column="column1">
                                @Html.DisplayFor(modelItem => item.Orden)
                            </td>
                            <td class="column100 column2" data-column="column2">
                                @Html.DisplayFor(modelItem => item.Nombre)
                            </td>
                            <td class="column100 column3" data-column="column3">
                                @Html.DisplayFor(modelItem => item.Tiempo)
                            </td>
                            <td class="column100 column4" data-column="column4">
                                @Html.DisplayFor(modelItem => item.IdAgencia)
                            </td>                        
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>